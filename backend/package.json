    "@types/jest": "^29.5.14",
    "@types/node": "^22.10.7",
    "esbuild": "^0.24.2",
    "eslint": "^9.18.0",
    "globals": "^15.14.0",
    "jest": "^29.7.0",
    "nodemon": "^3.1.9",
    "npm-check-updates": "^17.1.14",
    "rimraf": "^6.0.1",
    "supertest": "^7.0.0",
    "ts-jest": "^29.2.5",
    "ts-jest-mock-import-meta": "^1.2.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "typescript-eslint": "^8.21.0"
  }
