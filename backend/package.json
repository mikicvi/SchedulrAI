{
  "name": "schedulrai_backend",
  "version": "1.0.0",
  "description": "Backend services for SchedulrAI application.",
  "main": "dist/server.js",
  "type": "module",
  "scripts": {
    "start": "node dist/server.js",
    "build": "node esbuild.config.js",
    "lint": "eslint .",
    "dev": "nodemon --watch src --exec tsx src/server.ts",
    "ci": "npm install --omit=dev",
    "clean": "rimraf dist",
    "test": "jest --coverage",
    "test:watch": "jest --coverage --watch",
    "prod": "npm run clean && npm run build && npm run start",
    "update:deps": "ncu -u && npm install"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@langchain/community": "^0.3.26",
    "@langchain/core": "^0.3.33",
    "@langchain/ollama": "^0.1.4",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.7.9",
    "bcrypt": "^5.1.1",
    "chromadb": "^1.10.3",
    "chromadb-default-embed": "^2.13.2",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "langchain": "^0.3.12",
    "mongo": "^0.1.0",
    "ollama": "^0.5.12",
    "openai": "^4.80.0",
    "passport": "^0.7.0",
    "passport-local": "^1.0.0",
    "sequelize": "^6.37.5",
    "sqlite3": "^5.1.7",
    "winston": "^3.17.0"
